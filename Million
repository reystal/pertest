import java .lang.Math;
public class Million
{
    public static void main(String []args)
    {
        long Rich=0;
        long poor=0;
        for(int i=1;i<=30;i++)
        {
            Rich+=10;
            poor=poor+(long)Math.pow(2,i-1);
        }
       System.out.print(Rich+" "+poor);
    }
}
